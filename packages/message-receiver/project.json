{
  "name": "message-receiver",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/message-receiver/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "zip -r ../message-receiver.zip *"
        ],
        "cwd": "dist/packages/message-receiver"
      },
      "dependsOn": [{
        "projects": "self",
        "target": "transpile"
      }
      ]
    },
    "transpile": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "externalDependencies": "none",
        "outputPath": "dist/packages/message-receiver",
        "main": "packages/message-receiver/src/main.ts",
        "tsConfig": "packages/message-receiver/tsconfig.app.json",
        "assets": ["packages/message-receiver/src/assets"],
        "isolatedConfig": true,
        "webpackConfig": "packages/message-receiver/webpack.config.js"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "message-receiver:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "message-receiver:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/message-receiver/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/message-receiver/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
